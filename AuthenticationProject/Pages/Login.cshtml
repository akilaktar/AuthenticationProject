@page
@model AuthenticationProject.Pages.LoginModel
@{
}
<body>
    <div class="row">
        <div class="col-md-9">
            <form method="post" class="form">
                <div class="form-group">
                    <label asp-for="Model.EmailId"></label>
                    <input type="text" asp-for="Model.EmailId" />
                    <span asp-validation-for="Model.EmailId" class="text-danger"></span>
                </div>
                <br />
                <div class="form-group">
                    <label asp-for="Model.Password"></label>
                    <input type="password" asp-for="Model.Password" />
                    <span asp-validation-for="Model.Password" class="text-danger"></span>
                </div>
                <br />
              
                <button type="submit">Login</button>
            </form>
        </div>
    </div>
</body>
